#include <iostream>
using namespace std;
int main ()
{
int n;
cout<<"Enter the input:-"<<endl;
cin>>n;
{
for(int i=0;i<n-4;i++)
{
for(int j=0;j<2*n;j++)
{
cout<<"*";
}
cout<<endl;
}
for(int i=0;i<n-1;i++)
{
for(int j=0;j<n-4;j++)
{
cout<<"*";
} 
for(int j=0;j<n+3;j++)
{
	cout<<" ";
	}
	for(int j=0;j<n-4;j++)
	{
		cout<<"*";
		}
cout<<endl;
}
for(int i=0;i<n-4;i++)
{
for(int j=0;j<2*n;j++)
{
cout<<"*";
}
cout<<endl;
}
}
return 0;
}
